{
	"type": "sqlite",
	"database": "db.boilerplate.sqlite3",
	"synchronize": true,
	"logging": false,
	"entities": [
		"src/entities/**/*.ts"
	],
	"migrations": [
		"src/migrations/**/*.ts"
	],
	"subscribers": [
		"src/subscribers/**/*.ts"
	],
	"cli": {
		"entitiesDir": "src/entity",
		"migrationsDir": "src/migration",
		"subscribersDir": "src/subscriber"
	}
}